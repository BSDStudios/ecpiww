INC= -I .   -I ../include

ecpiww: 		 	ecpiww.o wmbus.o 
					gcc -o ecpiww ecpiww.o wmbus.o -lpthread -ldl
				
ecpiww.o:			../src/energycam/ecpiww.c ../include/energycam/ecpiww.h 
					gcc $(INC) -c ../src/energycam/ecpiww.c
							
wmbus.o:			../src/energycam/wmbus.c
					gcc $(INC) -pthread -c ../src/energycam/wmbus.c

clean: 			
				@rm ecpiww ecpiww.o wmbus.o
				@echo Clean done
